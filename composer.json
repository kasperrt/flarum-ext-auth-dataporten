    "type": "flarum-extension",
    "keywords": ["authentication"],
    "license": "MIT",
    "version": "v0.1.0-beta.2",
    "authors": [
        {
            "name": "Simon Skr√∏dal",
            "email": "simon.skrodal@uninett.no"
        }
    ],
    "support": {
